<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MCA.McaQPaper.McaSem1QPaper">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/mca_sem1_toolbar_qpaper"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/blue"
        app:title="MCA Semester - 1"
        app:titleTextColor="@color/black"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"/>
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/qpaperbackground"
                android:layout_margin="10dp"
                tools:ignore="UselessParent">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/find_the_semester_nand_explore_your_nexam_question_npaper"
                    android:textStyle="bold"
                    android:textColor="@color/black"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="40dp"
                    android:layout_marginBottom="40dp"
                    android:fontFamily="sans-serif-black"
                    android:textSize="20sp"/>
                <ImageView

                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_marginStart="60dp"
                    android:layout_marginTop="40dp"
                    android:src="@drawable/exam"
                    android:contentDescription="@string/todo" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="15dp"
                android:background="@drawable/border_layout"
                android:orientation="vertical">
                <TextView
                    android:layout_width="match_parent"
                    android:gravity="center"
                    android:layout_height="wrap_content"
                    android:text="@string/choose_your_subject"
                    android:textStyle="bold"
                    android:textColor="@color/black"

                    android:layout_marginTop="20dp"
                    android:fontFamily="sans-serif-black"
                    android:textSize="20sp"/>

                <LinearLayout
                    android:id="@+id/mca_sem1_qpaper_java"
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:orientation="horizontal"
                    android:layout_marginLeft="10dp"
                    android:layout_marginBottom="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginTop="40dp"
                    android:onClick="BottomOnclick"
                    android:background="@drawable/border_layout">

                    <TextView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_marginStart="5dp"
                        android:background="@drawable/symbol_layout_logo"
                        android:fontFamily="sans-serif-black"
                        android:gravity="center"
                        android:text="@string/a"
                        android:textColor="@color/white"
                        android:textSize="14sp" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="@string/java"
                        android:textStyle="bold"
                        android:textColor="@color/black"
                        android:gravity="fill"
                        android:layout_marginStart="5dp"
                        android:fontFamily="sans-serif-black"
                        android:textSize="14sp"/>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/mca_sem1_qpaper_php"
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:orientation="horizontal"
                    android:layout_margin="10dp"
                    android:onClick="BottomOnclick"
                    android:background="@drawable/border_layout">

                    <TextView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_marginStart="5dp"
                        android:background="@drawable/symbol_layout_logo"
                        android:fontFamily="sans-serif-black"
                        android:gravity="center"
                        android:text="@string/o"
                        android:textColor="@color/white"
                        android:textSize="14sp" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="@string/open_source_web_programming_using_php"
                        android:textStyle="bold"
                        android:textColor="@color/black"
                        android:gravity="fill"
                        android:layout_marginStart="5dp"
                        android:fontFamily="sans-serif-black"
                        android:textSize="14sp"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/mca_sem1_qpaper_adbms"
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:orientation="horizontal"
                    android:layout_margin="10dp"
                    android:onClick="BottomOnclick"
                    android:background="@drawable/border_layout">

                    <TextView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_marginStart="5dp"
                        android:background="@drawable/symbol_layout_logo"
                        android:fontFamily="sans-serif-black"
                        android:gravity="center"
                        android:text="@string/a"
                        android:textColor="@color/white"
                        android:textSize="14sp" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="@string/advance_dbms_administration"
                        android:textStyle="bold"
                        android:textColor="@color/black"
                        android:gravity="fill"
                        android:layout_marginStart="5dp"
                        android:fontFamily="sans-serif-black"
                        android:textSize="14sp"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/mca_sem1_qpaper_se"
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:orientation="horizontal"
                    android:layout_margin="10dp"
                    android:onClick="BottomOnclick"
                    android:background="@drawable/border_layout">

                    <TextView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_marginStart="5dp"
                        android:background="@drawable/symbol_layout_logo"
                        android:fontFamily="sans-serif-black"
                        android:gravity="center"
                        android:text="@string/s"
                        android:textSize="14sp"
                        android:textColor="@color/white" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="@string/software_engineering"
                        android:textStyle="bold"
                        android:textColor="@color/black"
                        android:gravity="fill"
                        android:layout_marginStart="5dp"
                        android:fontFamily="sans-serif-black"
                        android:textSize="14sp"/>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/mca_sem1_qpaper_dcn"
                    android:layout_width="match_parent"
                    android:layout_height="60dp"
                    android:orientation="horizontal"
                    android:layout_margin="10dp"
                    android:onClick="BottomOnclick"
                    android:background="@drawable/border_layout">

                    <TextView
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_marginStart="5dp"
                        android:background="@drawable/symbol_layout_logo"
                        android:fontFamily="sans-serif-black"
                        android:gravity="center"
                        android:text="@string/d"
                        android:textColor="@color/white"
                        android:textSize="14sp"
                        tools:ignore="TextContrastCheck" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="@string/dcn"
                        android:textStyle="bold"
                        android:textColor="@color/black"
                        android:gravity="fill"
                        android:layout_marginStart="5dp"
                        android:fontFamily="sans-serif-black"
                        android:textSize="14sp"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>


</LinearLayout>